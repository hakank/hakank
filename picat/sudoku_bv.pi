sudoku(Data, X) =>
   N = Data.len,
   N2 = sqrt(N).round,

   X = {make_bv_array(N,1,N) : _ in 1..N},
   foreach(I in 1..N, J in 1..N, nonvar(Data[I,J]))
     bv_eq(X[I,J],Data[I,J])
   end,
   

   foreach(Row in X) bv_all_different(Row) end,
   foreach(Column in transpose(X)) bv_all_different(Column) end,
   foreach(I in 1..N2..N, J in 1..N2..N)
      bv_all_different([X[I+K,J+L] : K in 0..N2-1, L in 0..N2-1])
   end,

   solve(X).


