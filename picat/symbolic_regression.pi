% logical and
% (from my JGAP implementation AndD)
eval('and'(X,Y)) = Val =>
    XVal = eval(X),
    YVal = eval(Y),
    B1 = cond(XVal < 1,false,true),
    B2 = cond(YVal < 1,false,true),
    Val = cond((B1,B2),1, 0).

% logical or
% (from my JGAP implementation OrD)
eval('or'(X,Y)) = Val =>
    XVal = eval(X),
