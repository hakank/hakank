% mod_replace:
% if X mod Y != 0 then return X mod Y else return Z
%
eval('mod_replace'(X,Y)) = Val =>
    XVal = eval(X),
    YVal = eval(Y),
    Res = XVal mod YVal,
    % The value to replace 0 with
    ZVal = get_global_map().get(mod_replace_value,1),
    Val = cond(Res != 0,Res, ZVal).

