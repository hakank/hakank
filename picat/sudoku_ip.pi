run_all_sizes(ProblemSize) ?=>
    % nolog,
    Map = get_global_map(),
    Map.put(successes,[]),
    Map.put(misses,[]),    
    if member(cp,sys.loaded_modules()) then
      println("Please use the SAT solver for solving the 25x25 problems!")
    end,
    Timeout = 60_000, % millis
    println(timeout_seconds=(Timeout//1000)),

    % failure driven loop
    member(P, 0..90),
    problem(P,X),
    Size1 = length(X),
    Size = ceiling(sqrt(Size1)),
    Size == ProblemSize,
    println(problem=P),
    println(size=Size),
    print_board(X),
    flush(stdout),
    [Time,_Backtracks,Status] = time2f($sudoku(X,Sol),Timeout),
    if Status == success then
      print_board(Sol),
      TimeSec = Time/1000,
      println(time_sec=TimeSec),
      Map.put(successes,Map.get(successes) ++ [[p=P,time_sec=TimeSec]])
    else
      println(timeout),
      Map.put(misses,Map.get(misses) ++ [P])      
    end,
    nl,
    flush(stdout),
    fail,   
    nl.

run_all_sizes(_Size) =>
  Map = get_global_map(),
  Successes = Map.get(successes),
  println("Successes:"),
  foreach(Success in Successes)
    println(Success)
  end,
  Misses = Map.get(misses),
  println(misses=Misses),
  
  nl.


sudoku(Problem, Sol) =>
   % decision variables
   N = Problem.len,

   M = ceiling(sqrt(N)),
   println([n=N,m=M]),
   Ks = [I : I in 1..M..N], 

   % X[I,J,K] = 1 means cell [I,J] is assigned number K
   X = new_array(N,N,N),
   X :: 0..1,

   % for the output
   Sol = new_array(N,N),
   Sol :: 1..N,

   % constraints

   % assign pre-defined numbers using the "givens" 
   foreach(I in 1..N, J in 1..N, K in 1..N, nonvar(Problem[I,J]))
     if Problem[I,J] == K then
        X[I,J,K] #= 1
     end
   end,

   % each cell must be assigned exactly one number 
   foreach(I in 1..N, J in 1..N) 
       sum([X[I,J,K] : K in 1..N]) #= 1
   end,


   % cells in the same row must be assigned distinct numbers 
   foreach(I in 1..N, K in 1..N) 
      sum([X[I,J,K] : J in 1..N]) #= 1
   end,


   % cells in the same column must be assigned distinct numbers 
   foreach(J in 1..N, K in 1..N) 
      sum([X[I,J,K] : I in 1..N]) #= 1
   end,

   % cells in the same region must be assigned distinct numbers 
   foreach(I1 in Ks, J1 in Ks, K in 1..N) 
     sum([X[I,J,K] : I in I1..I1+M-1, J in J1..J1+M-1]) #= 1
   end,

   % the solution
   foreach(I in 1..N, J in 1..N) 
      Sol[I,J] #= sum([X[I,J,K] * K: K in 1..N])
   end,
   
   Vars = X ++ Sol,
   % Vars = X,
   % solve($[ff,split,limit(2)],Vars). % proving unicity
   solve($[constr,updown],Vars).
   % % for the output
   % Sol = new_array(N,N),
   % foreach(I in 1..N, J in 1..N) 
   %   Sol[I,J] #= sum([X[I,J,K] * K: K in 1..N])
   % end.



print_board(Board) =>
   N = Board.length,
   foreach(I in 1..N)
      foreach(J in 1..N)
         X = Board[I,J],
         if var(X) then printf(" %3w","_") else printf(" %3w", X) end
      end,
      nl
   end,
   nl.

%
% time2 + time_out as a function.
%
time2f(Goal,Timeout) = [End,Backtracks,Status] =>
    statistics(runtime,_),
    statistics(backtracks, Backtracks1),
    time_out(Goal,Timeout,Status),
    statistics(backtracks, Backtracks2),
    statistics(runtime, [_,End]),
    Backtracks = Backtracks2 - Backtracks1.




