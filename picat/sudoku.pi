go10 =>
    nolog,
    if member(cp,sys.loaded_modules()) then
      println("Please use the SAT solver for solving the 25x25 problems!")
    end,
    Timeout = 60_000, % millis
    println(timeout_seconds=(Timeout//1000)),
    Successes = [],
    Misses = [],
    foreach(P in 0..90)
      problem(P,X),
      Size1 = length(X),
      Size = ceiling(sqrt(Size1)),
      if Size == 5 then
        println(problem=P),
        println(size=Size),
        print_board(X),
        flush(stdout),
        [Time,_Backtracks,Status] = time2f($sudoku(Size, X),Timeout),
        if Status == success then
           print_board(X),
           TimeSec = Time/1000,
           println(time_sec=TimeSec),           
           Successes := Successes ++ [[p=P,time_sec=TimeSec]]
        else
           println(timeout),
           Misses := Misses ++ [P]
        end,
        nl,
        flush(stdout)
      end
    end,
    println("The following instances was solved"),
    foreach(S in Successes)
      println(S)
    end,
    nl,
    println("The following instances was not solved"),
    println(Misses),
    
    nl.

