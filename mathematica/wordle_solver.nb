Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"createFreq", "[", "words_", "]"}], " ", ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"t", ",", "map"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"map", " ", "=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Association", "[", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "5"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Do", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"t", "=", " ", 
            RowBox[{"StringTake", "[", 
             RowBox[{"w", ",", 
              RowBox[{"{", "i", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
           
           RowBox[{
            RowBox[{
             RowBox[{"map", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "[", 
             RowBox[{"[", "t", "]"}], "]"}], " ", "=", " ", 
            RowBox[{
             RowBox[{"Lookup", "[", 
              RowBox[{
               RowBox[{"map", "[", 
                RowBox[{"[", "i", "]"}], "]"}], ",", "t", ",", "0"}], "]"}], 
             "+", "1"}]}]}], ",", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "5"}], "}"}]}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"w", ",", "words"}], "}"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"Reverse", "@", 
         RowBox[{"Flatten", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Keys", "@", 
             RowBox[{"Reverse", "[", 
              RowBox[{"SortBy", "[", 
               RowBox[{"#", ",", "2"}], "]"}], "]"}]}], ",", " ", 
            RowBox[{"Complement", "[", 
             RowBox[{
              RowBox[{"Alphabet", "[", "]"}], ",", 
              RowBox[{"Keys", "[", " ", "#", "]"}]}], "]"}]}], "}"}], "]"}]}],
         "  ", "&"}], " ", "/@", " ", "map"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"Shorter", " ", "version"}], " ", "*)"}]}], "\[IndentingNewLine]", 

 RowBox[{
  RowBox[{
   RowBox[{"createFreq2", "[", "words_", "]"}], " ", ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "k", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"k", "=", "\[IndentingNewLine]", "    ", 
         RowBox[{"Keys", "[", 
          RowBox[{"SortBy", "[", 
           RowBox[{
            RowBox[{"Counts", "[", 
