Cell["\<\
Below is the code for the Wordle solve.  It has a method for each of the \
three constraints:
 * correct position (checkCorrectPos[])
 * correct chars (checkCorrectChars[]))
 * not in word (checkNotInWord[])
 and for the scoring method mentioned above (sortCandidates[]).

Some of the methods can probably be done neater, especially \
checkCorrectChars[].  

For sortCandidates[], see the section \[OpenCurlyDoubleQuote]Frequency tables \
for the scoring\[CloseCurlyDoubleQuote] above.\
\>", "Text",
 CellChangeTimes->{{3.859802817316573*^9, 3.8598028309201317`*^9}, {
   3.8598036228504457`*^9, 3.8598037559936666`*^9}, 3.859803889966661*^9, {
   3.859820226863172*^9, 3.859820227592197*^9}, {3.859851398074971*^9, 
   3.859851503295739*^9}},ExpressionUUID->"841c69b8-bf99-4831-9d29-\
b4555d818cfe"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"helper", ":", " ", 
    RowBox[{"zipping", " ", "two", " ", "lists"}]}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"zip", "[", 
      RowBox[{"a_List", ",", "b_List"}], "]"}], " ", ":=", " ", 
     RowBox[{"Transpose", "[", 
      RowBox[{"{", 
       RowBox[{"a", ",", "b"}], "}"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Character", " ", "in", " ", "correct", " ", "position"}], " ", 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"checkCorrectPos", "[", 
     RowBox[{"chars_", ",", "word_"}], "]"}], " ", ":=", " ", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"StringMatchQ", "[", 
       RowBox[{"word", ",", 
        RowBox[{"RegularExpression", "@", "chars"}]}], "]"}]}], 
     "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Character", " ", "in", " ", "word", " ", "but", " ", "in", " ", "wrong", 
     " ", "position"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", "helper", " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"checkChars1", "[", 
      RowBox[{"word_", ",", "chars_"}], "]"}], " ", ":=", 
     RowBox[{
      RowBox[{
       RowBox[{"StringContainsQ", "[", 
        RowBox[{"word", ",", "#"}], "]"}], " ", "&"}], "  ", "/@", " ", 
      RowBox[{"StringSplit", "[", 
       RowBox[{"chars", ",", "\"\<\>\""}], "]"}]}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"checkCorrectChars", "[", 
     RowBox[{"chars_", ",", "word_"}], "]"}], " ", ":=", " ", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"pos", ",", "zs", ",", 
        RowBox[{"len", "=", 
         RowBox[{"StringLength", "[", "word", "]"}]}], ",", "t1"}], "}"}], 
      ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"zs", " ", "=", " ", 
        RowBox[{"zip", "[", 
         RowBox[{
          RowBox[{"StringSplit", "[", 
           RowBox[{"word", ",", "\"\<\>\""}], "]"}], ",", "chars"}], "]"}]}], 
       ";", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"all", " ", "chars", " ", "are", " ", "in", " ", "word"}], 
        " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"t1", " ", "=", " ", 
        RowBox[{"And", " ", "@@", 
         RowBox[{"Flatten", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"#", " ", "==", " ", "\"\<\>\""}], " ", "||", " ", 
               RowBox[{"checkChars1", "[", 
                RowBox[{"word", ",", "#"}], "]"}]}], ")"}], "  ", "&"}], " ", 
            "/@", " ", "chars"}], ",", "1"}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
        "no", " ", "chars", " ", "is", " ", "in", " ", "correct", " ", 
         "position"}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{"t1", ",", 
         RowBox[{"And", " ", "@@", " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"#", "[", 
                RowBox[{"[", "2", "]"}], "]"}], " ", "==", " ", "\"\<\>\""}], 
              " ", "||", " ", 
              RowBox[{"Not", "[", 
               RowBox[{"StringContainsQ", "[", 
                RowBox[{
                 RowBox[{"#", "[", 
                  RowBox[{"[", "1", "]"}], "]"}], ",", 
                 RowBox[{"#", "[", 
                  RowBox[{"[", "2", "]"}], "]"}]}], " ", "]"}], "]"}]}], 
             "&"}], " ", "/@", " ", "zs"}], ")"}]}]}], "]"}]}]}], 
     "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "No", " ", "character", " ", "in", " ", "chars", " ", "are", " ", "in", 
     " ", "the", " ", "word"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"checkNotInWord", "[", 
     RowBox[{"chars_", ",", "word_"}], "]"}], " ", ":=", " ", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"And", " ", "@@", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"chars", " ", "==", " ", "\"\<\>\""}], " ", "||", " ", 
           "\[IndentingNewLine]", 
           RowBox[{"Not", "[", 
            RowBox[{"StringContainsQ", "[", 
             RowBox[{"word", ",", " ", "#"}], "]"}], "]"}]}], " ", "&"}], " ",
          "/@", 
         RowBox[{"StringSplit", "[", 
          RowBox[{"chars", ",", "\"\<\>\""}], "]"}]}], ")"}]}]}], 
     "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Sort", " ", "the", " ", "candidate", " ", 
     RowBox[{"words", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"sortCandidates", "[", "candidates_", "]"}], "  ", ":=", " ", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"alphas", ",", "scores"}], "}"}], ",", "\[IndentingNewLine]", 
      " ", 
      RowBox[{"(*", " ", 
       RowBox[{
       "This", " ", "is", " ", "the", " ", "frequency", " ", "order", " ", 
        RowBox[{"(", "reversed", ")"}], " ", "for", " ", "each", " ", 
        "positions", " ", "\[IndentingNewLine]", "   ", "of", " ", "the", " ", 
        RowBox[{"word", "."}]}], "   ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"alphas", "=", 
        RowBox[{"{", 
         RowBox[{
         "\"\<xzyjkquinovhewlrmdgfaptbcs\>\"", ",", "\[IndentingNewLine]", 
          "               ", "\"\<zqfkgxvbsdymcwptnhulieroaj\>\"", ",", 
          "\[IndentingNewLine]", "               ", 
          "\"\<qjhzkxfwyvcbpmgdstlnrueoia\>\"", ",", "\[IndentingNewLine]", 
          "               ", "\"\<qjxyzbwhfvpkmdguotcrilasne\>\"", ",", 
          "\[IndentingNewLine]", "              ", 
          "\"\<qvjuzxbiwfcsgmpoakdnhlrtye\>\""}], "}"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"SortBy", "[", 
         RowBox[{"candidates", ",", 
          RowBox[{
           RowBox[{"scoreWord", "[", 
            RowBox[{"#", ",", "alphas"}], "]"}], " ", "&"}]}], "]"}], "//", 
        "Reverse"}]}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
     "]"}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Score", " ", "a", " ", "word"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"scoreWord", "[", 
     RowBox[{"word_", ",", "alphas_"}], "]"}], " ", ":=", " ", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"score1", ",", "score2"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{
        "The", " ", "score", " ", "is", " ", "the", " ", "total", " ", "of", 
         " ", "its", " ", "positions", " ", "in", " ", "the", " ", "alphas", 
         " ", "strings", " ", "above"}], " ", "+", " ", "\[IndentingNewLine]",
         "   ", 
        RowBox[{
        "a", " ", "100", " ", "points", " ", "credit", " ", "if", " ", "the", 
         " ", "word", " ", "has", " ", "unique", " ", 
         RowBox[{"characters", "."}]}]}], "  ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"score1", " ", "=", " ", 
        RowBox[{"Total", "@", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"StringPosition", "[", 
              RowBox[{
               RowBox[{"alphas", "[", 
                RowBox[{"[", "#", "]"}], "]"}], ",", 
               RowBox[{"StringTake", "[", 
                RowBox[{"word", ",", 
                 RowBox[{"{", "#", "}"}]}], "]"}]}], "]"}], "[", 
             RowBox[{"[", 
              RowBox[{"1", ",", "1"}], "]"}], "]"}], "  ", "&"}], " ", "/@", 
           "  ", 
